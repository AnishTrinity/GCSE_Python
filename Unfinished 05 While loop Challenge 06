num = int(input("Enter a number between 10 and 20:"))


if num >10 and num<20:
  print("Thank you")
  exit()
elif num>20:
  print("Too high")
else:
  print("Try again")
  int(input("Try again. Enter a number between 10 and 20:"))
  while num>20 and num<10 :
    print("Try again")
    int(input("Enter a number between 10 and 20:"))
    
