range = range(10,20)
num = int(input("Enter a number between 10 and 20:"))

while num not in range :
  if num in range:
    print("Thank you")
    exit()
  else:
    print("Try again")
    if num<10:
      print("Too low")
    else:
      print("Too high")
    num = int(input("Try again. Enter a number between 10 and 20:"))
    
